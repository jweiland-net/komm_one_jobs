{
	"name": "jweiland/komm-one-jobs",
	"type": "typo3-cms-extension",
	"description": "Use the komm.ONE jobs API to display jobs",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"komm.one",
		"jobs"
	],
	"homepage": "https://jweiland.net",
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "sfroemken@jweiland.net",
			"role": "Lead Developer"
		}
	],
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/komm_one_jobs/issues",
		"source": "https://github.com/jweiland-net/komm_one_jobs"
	},
	"require": {
		"ext-simplexml": "*",
		"typo3/cms-core": "^12.4.31 || ^13.4.12"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\KommOneJobs\\": "Classes"
		}
	},
	"replace": {
		"typo3-ter/komm_one_jobs": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "komm_one_jobs"
		}
	}
}
